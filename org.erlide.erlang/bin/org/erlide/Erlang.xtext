grammar org.erlide.Erlang with org.eclipse.xtext.common.Terminals hidden(WS, COMMENT)

generate erlang "http://www.erlide.org/Erlang"

Module:
	forms+=Form*
	;
	
Form:
    Attribute |
    Function
    ;	

Attribute:
    '-' ATOM '.'
;

Function:
    ATOM '.'
;
	
terminal ATOM: ('a'..'z') ('a'..'z'|'A'..'Z'|'_'|'0'..'9')*;
terminal VARIABLE: ('A'..'Z'|'_') ('a'..'z'|'A'..'Z'|'_'|'0'..'9')*;
terminal COMMENT: '%' !('\n'|'\r')* ('\r'? '\n')?;
